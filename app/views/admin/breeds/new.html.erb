<h1>New breed</h1>

<% form_for :breed,
		    :url => admin_breeds_path(@breed),
            :html => { :method => :post } do |f| %>
  <%= f.error_messages %>

	<%= f.label :name %>
	<%= f.text_field :name %>

  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', admin_breeds_path %>