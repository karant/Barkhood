<%- unless groups.empty? -%>
  <h2><%= title %></h2>
  <ul class="grid contacts small">
    <%- groups.each do |group| -%>
      <%= render :partial => 'shared/contact', :locals => {:contact => group} %>
    <%- end -%>
    <li class="more">
      <%= link_to "See all &raquo;",
          link == "admin" ?
              admin_groups_dog_path(dog) : groups_dog_path(dog) %>
    </li>
  </ul>
<%- end -%>
