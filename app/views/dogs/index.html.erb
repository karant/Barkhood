<%= render :partial => 'searches/address' %>
<%= @map.div(:width => 704, :height => 400) %>
<%- column_div :type => :primary do -%>
  <% if @dogs.empty? -%>
  	<br />
	No dogs were found near this address!
  <% else -%>
    <h2>Dogs (<%= @dogs.total_entries %>)</h2>
    <%= will_paginate %>
    <ul class="list dogs">
      <%= render :partial => @dogs %>
    </ul>
    <%= will_paginate %>
  <% end -%>
<%- end -%>

<%- column_div :type => :secondary do -%>
  <%= render :partial => 'searches/box' %>
  <%= render :partial => 'shared/minifeed' %>
<%- end -%>