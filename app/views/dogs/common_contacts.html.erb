<%- column_div :type => :primary do -%>
<h2>Common contacts with <%= @dog.name %></h2>
<% unless @common_contacts.empty? -%>
  <%= will_paginate(@common_contacts) %>
  <ul class="list dogs">
    <%= render :partial => 'dogs/dog',
               :collection => @common_contacts %>
  </ul>
  <%= will_paginate(@common_contacts) %>
<% else %>
  <h2>No contacts in common</h2>
<% end -%>

<%- end -%>

<%- column_div :type => :secondary do -%>
  <%= render :partial => 'shared/minifeed' %>
<%- end -%>
